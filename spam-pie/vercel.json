{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "env": {
      "NODE_VERSION": "18.x"
    },
    "build": {
      "env": {
        "NODE_VERSION": "18.x"
      },
      "installCommand": "npm install --legacy-peer-deps",
      "command": "npm run build",
      "outputDirectory": "build"
    }
  }
  